[General]
// Resolution of game (integer)
GameWindowWidth=1280	
GameWindowHeight=720
// Toggle fullscreen/window mode (boolean) (Only used on non-arcade builds)
FullScreen=false
// Toggle VSync (boolean)
VSync=false
// Toggle frame cap (boolean)
FixedTimeStep=true

// Number of players (integer 1-4)
NumberOfPlayers=4

// Control Scheme (1 = Keyboard, 2 = Arcade) (Only used on non-arcade builds)
ControlScheme=1

[Sound]
// From 0.0 - 1.0 (silence to current device level)
MasterVolume=0.5
FXVolume=0.75
MusicVolume=0.25

// Px = Player ID
// Left Rocket/Boost/Right Rocket (Must be uppercase)
// Keys seperated by commas
[Keyboard Keys]
P1=Q,W,E
P2=Z,X,C
P3=M,OemComma,OemPeriod
P4=P,OemOpenBrackets,OemCloseBrackets

[Arcade Keys]
P1=LeftShift,Z,X
P2=W,I,K
P3=LeftControl,LeftAlt,Space
P4=A,S,Q

// All players use the same button scheme on gamepad
[Gamepad Keys]
Boost=B

// Debug controls
[Keyboard Debug Keys]
Skip=Enter
Restart=R
Quit=Escape

[Gamepad Debug Keys]
GPSkip=LeftShoulder
GPRestart=Start
GPQuit=Back

[Arcade Debug Keys]
Skip=D1
Restart=D2
Quit=D5


//	End of file
[End]
